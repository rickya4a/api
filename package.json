{
  "name": "api",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rickya4a/api.git"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require @babel/register --require dotenv/config",
    "start": "nodemon --exec babel-node --require ./node_modules/dotenv/config src/server.js",
    "start:ssh": "nodemon --exec babel-node --require ./node_modules/dotenv/config --tls-min-v1.0 src/server.js",
    "build": "babel ./src --out-dir dist",
    "serve": "node -r dotenv/config dist/server.js"
  },
  "nodemonConfig": {
    "delay": 2000,
    "env": {
      "NODE_ENV": "development"
    }
  },
  "author": "Ricky Aditya",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-base64": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "localStorage": "^1.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "mssql": "^5.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.0.2",
    "serve-index": "^1.9.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.0.1"
  }
}
